#include "main.h"

/**
 * rev_string - print string in reverse
 *
 * @s: the string to print
 *
 * Return: reversed string
 */

void rev_string(char *s)
{
	int i, j;

	i = 0;
	j = 0;
	while (*(s + i) != '\0')
	{
		i++;
	}
	/*iterate in reverse */

	while (i >= 0)
	{
		*(s + j) = *(s + i);
		i--;
		j++;
	}

}
